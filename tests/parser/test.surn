class Test {
    pub x: string;

    pub fn construct() {
        this.x = "hello";
    }

    pub fn print(): void {
        print('${this.x}, world!');
    }

    pub fn write(value): Self {
        var self = new Self();
        self.x = value;
        return self;
    }
}

fn main() {
    var test = new Test();
    test.print();
}